{
  "rewrites": [
    {
      "source": "/balance",
      "destination": "https://hduofjzsitoaujyjvuix.supabase.co/functions/v1/server/balance"
    },
    {
      "source": "/changebalance",
      "destination": "https://hduofjzsitoaujyjvuix.supabase.co/functions/v1/server/changebalance"
    },
    {
      "source": "/changebalance/slot",
      "destination": "https://hduofjzsitoaujyjvuix.supabase.co/functions/v1/server/changebalance/slot"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(balance|changebalance)(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Accept"
        }
      ]
    }
  ]
}